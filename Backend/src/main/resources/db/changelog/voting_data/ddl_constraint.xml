<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
<changeSet id="b13377e2-41e6-4806-a583-67ff49a4ae3d" author="aminoiu">
    <addForeignKeyConstraint schemaName="voting_data"
                             baseColumnNames="admin_id"
                             baseTableName="votings_data"
                             constraintName="fk_admin_id_id"
                             referencedColumnNames="admin_id"
                             referencedTableName="admin"
                             onDelete="CASCADE"/>
    <addForeignKeyConstraint schemaName="voting_data"
                             baseColumnNames="voting_id"
                             baseTableName="casted_vote"
                             constraintName="fk_voting_id"
                             referencedColumnNames="voting_id"
                             referencedTableName="votings_data"
                             onDelete="CASCADE"/>
    <addForeignKeyConstraint schemaName="voting_data"
                             baseTableName="users_role"
                             baseColumnNames="user_id"
                             constraintName="FK_USER_ID"
                             referencedTableName="users"
                             referencedColumnNames="id"
                             onDelete="CASCADE"/>
    <addForeignKeyConstraint schemaName="voting_data"
                             baseTableName="users_role"
                             baseColumnNames="role_id"
                             constraintName="FK_ROLE_ID"
                             referencedTableName="roles"
                             referencedColumnNames="id"
                             onDelete="CASCADE"/>
    <addForeignKeyConstraint schemaName="voting_data"
                             baseTableName="admin"
                             baseColumnNames="user_id"
                             constraintName="FK_USER_ID_ADMIN"
                             referencedTableName="users"
                             referencedColumnNames="id"
                             onDelete="CASCADE"/>
    <addForeignKeyConstraint schemaName="voting_data"
                             baseTableName="voter"
                             baseColumnNames="user_id"
                             constraintName="FK_USER_ID_VOTER"
                             referencedTableName="users"
                             referencedColumnNames="id"
                             onDelete="CASCADE"/>
    <addForeignKeyConstraint schemaName="voting_data"
                             baseTableName="candidate"
                             baseColumnNames="user_id"
                             constraintName="FK_USER_ID_CANDIDATE"
                             referencedTableName="users"
                             referencedColumnNames="id"
                             onDelete="CASCADE"/>
</changeSet>
</databaseChangeLog>
